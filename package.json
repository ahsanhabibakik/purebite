{
  "name": "purebite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:clean": "rm -rf .next && npm run build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "npm run test:coverage && node scripts/coverage-report.js",
    "test:coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "test:coverage:ci": "CI=true npm run test:coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:all:report": "npm run test:coverage:report && npm run test:e2e && npm run test:e2e:report",
    "playwright:install": "playwright install",
    "coverage:check": "node scripts/coverage-report.js",
    "coverage:badge": "node scripts/coverage-report.js && cat coverage-reports/coverage-badge.json",
    "postinstall": "prisma generate",
    "fix:prisma": "node scripts/fix-prisma.js"
  },
